PLTF_CPPFLAGS += -DHAS_HW_MELIBU_TRIM=$(HAS_HW_MELIBU_TRIM)
PLTF_CPPFLAGS += -DHAS_HW_NO_EN_AAIN=$(HAS_HW_NO_EN_AAIN)
PLTF_CPPFLAGS += -DHAS_SW_MELIBU_EE_CTRL_CFG=$(HAS_SW_MELIBU_EE_CTRL_CFG)
